# preconfigure fragment for sh.

case "$machine" in
sh3eb)		base_machine=sh
		machine=sh/be/sh3
		with_fp_cond="defined __SH_FPU_ANY__"
		;;
sh3)		base_machine=sh
		machine=sh/le/sh3
		with_fp_cond="defined __SH_FPU_ANY__"
		;;
sh4eb)		base_machine=sh
		machine=sh/be/sh4
		with_fp_cond="defined __SH_FPU_ANY__"
		;;
sh4)		base_machine=sh
		machine=sh/le/sh4
		with_fp_cond="defined __SH_FPU_ANY__"
		;;
esac
